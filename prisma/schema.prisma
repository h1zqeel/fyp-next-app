generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URI")
}


model HospitalRequest{
  id Int @id @default(autoincrement())
  name String 
  email String @unique
  hospitalName String
  hospitalEmail String
  plan Int
  phone String
  hospitalPhone String
  why String
  approved Int @default(0)
}

model userAdmin {
  id Int @id @default(autoincrement())
  email String @unique
}